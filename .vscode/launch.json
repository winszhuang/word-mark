{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Debug App",
        "url": "http://localhost:5173",
        "enableContentValidation": false,
        "webRoot": "${workspaceFolder}/frontend/src",
        "sourceMapPathOverrides": {
          "webpack:///./src/*": "${webRoot}/*"
        },
        "runtimeArgs": ["--remote-debugging-port=9222"],
        // "sourceMaps": true
      }
    ]
  }